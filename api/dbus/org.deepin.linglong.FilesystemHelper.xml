<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "https://specifications.freedesktop.org/dbus/introspect-latest.dtd">
<node>
  <interface name="org.deepin.linglong.FilesystemHelper">
    <method name="Mount">
      <arg name="source" type="s"/>
      <arg name="mountPoint" type="s"/>
      <arg name="fsType" type="s"/>
      <arg name="options" type="a{sv}"/>
      <annotation name="org.qtproject.QtDBus.QtTypeName.In3" value="QVariantMap"/>
    </method>
    <method name="Umount">
      <arg name="mountPoint" type="s"/>
      <arg name="options" type="a{sv}"/>
      <annotation name="org.qtproject.QtDBus.QtTypeName.In1" value="QVariantMap"/>
    </method>
  </interface>
</node>
