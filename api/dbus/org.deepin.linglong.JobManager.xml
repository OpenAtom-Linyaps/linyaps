<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
        "https://specifications.freedesktop.org/dbus/introspect-latest.dtd">
<node>
    <interface name="org.deepin.linglong.JobManager">
        <method name="List">
            <arg name="JobIdList" type="as" direction="out"/>
        </method>
        <method name="Start">
            <arg name="JobId" type="s" direction="in"/>
        </method>
        <method name="Stop">
            <arg name="JobId" type="s" direction="in"/>
        </method>
        <method name="Cancel">
            <arg name="JobId" type="s" direction="in"/>
        </method>
    </interface>
</node>