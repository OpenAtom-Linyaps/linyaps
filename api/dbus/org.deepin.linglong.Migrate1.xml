<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "https://specifications.freedesktop.org/dbus/introspect-latest.dtd">
<node>
    <interface name="org.deepin.linglong.Migrate1">
        <method name="WaitForAvailable">
            <annotation name="org.freedesktop.DBus.Description"
                value="This method is a blocking method that retruns only only when package manager is available." />
        </method>
        <signal name="MigrateDone">
            <arg name="code" type="i" />
            <arg name="message" type="s" />
        </signal>
    </interface>
</node>
